            </div><br>
            <div id="submit_button">
              <input type="button" value="Log in!">
            </div>
          </form>
        </div>
        <div id="main_container">
          <div id="button_bar">
            <div class="button">Test</div>
            <div class="button">Test</div>
            <div class="button">Test</div>
            <div class="button">Test</div>
          </div>
            <div id="canvasDiv">
                <canvas id="canvas" width="800" height="550" tabindex="1">
                <div id="health_bar_div">
                    <div id="health_bar"></div>
                </div>
                </canvas>
            </div>
        </div>
    </body>
</html>